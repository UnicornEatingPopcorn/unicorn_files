        /**
         * set font-size
         */
        function setFontSize(identifier) {
            if(identifier !== undefined) {
                var textElement = document.querySelector('.sticky__footer__content__font--main--bold');
                switch (identifier) {
                    case "S":
                        return "sticky__footer__content__font--small"
                        break;
                    case "M":
                        return "sticky__footer__content__font--medium"
                        break;
                    case "L":
                        return "sticky__footer__content__font--large"
                        break;
                    default:
                        return "sticky__footer__content__font--medium"
                }
            }
        }

        /**
         * set mobile font-suisse
         */
        function setFontSuisse(identifier) {
            if(identifier !== undefined) {
                var fontSuisse = document.querySelector('.sticky__footer__content__font--main--bold');
                if (identifier === true) {
                    return "sticky__footer__content__font--suisse"
                }
                else { return 0 }
            }   
        }

        var sendProfile = function(counter) {
            var params = {};
            params['profile.'+window.webalyse.target.footers[0].cappingProfile] = counter;
            adobe.target.getOffer({
                'mbox': 'profile-mbox',
                'params': params,
                'success': function(offer) {
                    conlog('send mbox profile - ok');
                },
                'error': function(status, error) {
                    conlog('send mbox profile - error');
                    console.error('Error', status, error);
                }
            });
        }

        /**
         * STORAGE
         */
        function hasLocalStorage() {
            var has = typeof window.sessionStorage === 'object' && typeof window.sessionStorage.setItem === 'function' && typeof window.localStorage.getItem;
            if(!has) conlog('ERROR: no local storage');
            return has;
        }

        function getFooterStorage() {
            var footerStorageJSON = {'state': 'new', 'time': new Date().getTime()};

            if(hasLocalStorage()) {
                var footerStorage = window.localStorage.getItem(window.webalyse.target.footers[0].cappingProfile);
                if(footerStorage === null) {
                    conlog('footer state not set yet, setting');
                    setFooterStorage();
                    footerStorage = window.localStorage.getItem(window.webalyse.target.footers[0].cappingProfile);
                }

                try {
                    footerStorageJSON = JSON.parse(footerStorage);
                    // check daily capping
                    var now = new Date();
                    var setTime = new Date(footerStorageJSON.time);
                    if( setTime.getDate() !== now.getDate() ||
                        setTime.getMonth() !== now.getMonth() ||
                        setTime.getFullYear() !== now.getFullYear()
                    ) {
                        conlog('daily cap removed');
                        var dateTime = new Date().getTime();
                        setFooterStorage('new',dateTime);
                        sendProfile(0);
                        window.webalyse.target.footers[0].cappingValue = 0;
                        footerStorageJSON = {'state': 'new', 'time': dateTime};
                    }
                }
                catch(e) {
                    conlog('invalide local storage');
                }
            }

            return footerStorageJSON;
        }

        function setFooterStorage(state, time) {
            var footerStorageJSON = {'state': 'new', 'time': new Date().getTime()};

            if(hasLocalStorage()) {
                var footerStorage = window.localStorage.getItem(window.webalyse.target.footers[0].cappingProfile);
                if(footerStorage === null) {
                    conlog('footer state not set yet, setting');
                }
                else {
                    try {
                        footerStorageJSON = JSON.parse(footerStorage);
                    }
                    catch(e) {
                        conlog('invalide local storage');
                    }
                }
                if(typeof state !== 'undefined' && state !== null) footerStorageJSON.state = state;
                if(typeof time !== 'undefined' && time !== null) footerStorageJSON.time = time;

                window.localStorage.setItem(window.webalyse.target.footers[0].cappingProfile,JSON.stringify(footerStorageJSON));
            }
        }

        /**
         * footer state manipulation
         */
        function getFooterState() {
            return getFooterStorage().state;
        }
        function setFooterState(state) {
            if(hasLocalStorage()) {
                conlog('saving state '+state+' of footer');
                setFooterStorage(state);
            }
        }

