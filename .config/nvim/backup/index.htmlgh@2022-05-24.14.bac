            var adSlotMobile = document.getElementById('iqadtile1');
            // if ((taboola !== null || taboolaM !== null) && (footer !== null || footerM !== null)) {
            if (taboolaFound === false && taboola !== null && footer !== null) {
                // clearInterval(ival);
                taboolaFound = true;
                conlog('taboola found, adding scroll listener, desktop environment');
