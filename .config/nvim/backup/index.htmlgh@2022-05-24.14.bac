        var isScrolledUnderFooter = function(elem, footerHeight) {
            var docViewTop = window.scrollY;
            var docViewBottom = docViewTop + window.innerHeight;

            var elemTop = elem.getBoundingClientRect().top + window.scrollY;
            var elemBottom = elemTop + elem.offsetHeight;

            return ((elemTop < docViewBottom) && (elemBottom > docViewBottom-footerHeight));
        };

        function isScrolledIntoView(elem) {
            var docViewTop = window.scrollY;
            var docViewBottom = docViewTop + window.innerHeight;

            var elemTop = elem.getBoundingClientRect().top + window.scrollY;
            // var elemBottom = elemTop + elem.offsetHeight;

            // return ((elemBottom <= docViewBottom) && (elemTop >= docViewTop));
            return (elemTop < docViewBottom);
        }

        var addScrollListenerTaboola = function(elem, footer) {
            var footerHeight = footer.offsetHeight;
            window.addEventListener('scroll',function(){
                if (isScrolledUnderFooter(elem, footerHeight)) {
                    conlog('taboola scrolled into view');
                    if (!footer.classList.contains('wb-dnone')) footer.classList.add('wb-dnone');
                } else {
                    if (footer.classList.contains('wb-dnone')) footer.classList.remove('wb-dnone');
                }
            });
        };

        var addScrollListenerAdSlot = function(footer, adslot) {
            window.addEventListener('scroll',function(){
                var styleEl = document.getElementById('hide_footer');
                if (styleEl !== null) {
                    if (adslot !== null) {
                        if(isScrolledIntoView(adslot)) {
                            conlog('adslot found -> user started scrolling -> adslot in view -> showing sticky footer after 1 sec');
                            setTimeout(function(){
                                styleEl.parentNode.removeChild(styleEl);
                            },1000);
                        }
                    } else {
                        conlog('adslot not found -> showing sticky footer after 1 sec');
                        setTimeout(function(){
                            styleEl.parentNode.removeChild(styleEl);
                        },1000);
                    }
                }
            });
        };

