                conlog('adslot found, but we are doing nothing on desktop');
                // addScrollListenerAdSlot(footer, adSlotDesktop)
                var styleEl = document.getElementById('hide_footer');
                if (styleEl !== null) styleEl.parentNode.removeChild(styleEl);
            }
            if (footerFoundForAdTileListener === false && footerM !== null && adSlotMobile !== null) {
                footerFoundForAdTileListener = true;
